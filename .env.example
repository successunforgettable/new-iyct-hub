# Database (Supabase PostgreSQL)
# Get from: https://supabase.com/dashboard/project/_/settings/database
DATABASE_URL=postgresql://user:password@host:5432/database

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# JWT Authentication
# Generate with: openssl rand -base64 32
JWT_SECRET=your-jwt-secret-here
JWT_REFRESH_SECRET=your-jwt-refresh-secret-here

# Encryption
# Generate with: openssl rand -hex 32
ENCRYPTION_KEY=your-encryption-key-here

# AI Services
# Get from: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Inner DNA API
# Get from Inner DNA dashboard
INNER_DNA_API_URL=https://api.innerdna.com
INNER_DNA_API_KEY=your-innerdna-key-here

# OAuth (Add when credentials available)
# GOOGLE_CLIENT_ID=your-google-client-id
# GOOGLE_CLIENT_SECRET=your-google-client-secret
# FACEBOOK_APP_ID=your-facebook-app-id
# FACEBOOK_APP_SECRET=your-facebook-app-secret

# Payments (Add when ready - test mode)
# STRIPE_SECRET_KEY=sk_test_xxx
# STRIPE_WEBHOOK_SECRET=whsec_xxx
# CASHFREE_APP_ID=test-app-id
# CASHFREE_SECRET_KEY=test-secret-key
# CASHFREE_WEBHOOK_SECRET=webhook-secret

# AWS S3 (Optional - file storage)
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key
# AWS_S3_BUCKET=iyct-uploads
# AWS_REGION=us-east-1

# App Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
API_URL=http://localhost:3001

# Monitoring (Optional)
# SENTRY_DSN=your-sentry-dsn
